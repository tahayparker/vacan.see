generator client {
  provider = "prisma-client-js"
}

// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model Class {
  id        String    @id @default(cuid())
  SubCode   String
  Class     String
  Day       String
  StartTime String
  EndTime   String
  Room      String
  Teacher   String

  @@index([EndTime], map: "idx_classes_endtime")
  @@index([StartTime], map: "idx_classes_starttime")
  @@index([Day], map: "idx_classes_day")
}